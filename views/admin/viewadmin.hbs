<section>
    <div class="container">
        <div class="row mt-2">
            <a href="/admin/signup" class="btn btn-success ml-auto">Add admin</a>
        </div>
        <h2>Users</h2>
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                                        
                </tr>
            </thead>
            <tbody>
                {{#each admin}}
                <tr>
                    
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                                       
                    
                    {{!-- <td><img style="width: 50px;height: 50px" src="/images/worker/{{this._id}}.png" alt=""></td> --}}
                    <td>
                        
                        <a href="/admin/deleteadmin/{{this._id}}" class="btn btn-danger" onclick="return confirm('are you want to delete {{this.works.Name}} ?')">Delete</a>
                    </td>

                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>