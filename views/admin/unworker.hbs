<section>
    <div class="container">
        <h2>Workers</h2>
        <div class="row mt-2">
        </div>
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Address</th>
                    <th scope="col">Date of birth</th>
                    <th scope="col">Category</th>
                    <th scope="col">Experince</th>
                    <th scope="col">qualification</th>
                    <th scope="col">Aadhar</th>
                    <th scope="col">options</th>
                </tr>
            </thead>
            <tbody>
                {{#each content}}
                <tr>
                    
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                    <td>{{this.Phone}}</td>
                    <td>{{this.Address}}</td>
                    <td>{{this.DateOfBirth}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.Experience}}</td>
                    <td>{{this.Qualification}}</td>
                    
                    <td><img style="width: 50px;height: 50px" src="/images/unworker/{{this._id}}.png" alt=""></td>
                    <td>
                        
                        <a href="/admin/verifyworker/{{this._id}}" class="btn btn-danger"  >Verify</a>
                    </td>

{{!-- onclick="return confirm('are you want to delete {{this.works.Name}} ?')"--}}
                </tr> 
                {{/each}}
            </tbody>
        </table>
    </div>
</section>