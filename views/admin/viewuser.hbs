<section>
    <div class="container">
        {{!-- <div class="row mt-2">
            <a href="/admin/adduser" class="btn btn-success ml-auto">Add content</a>
        </div> --}}
        <h2>Users</h2>
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Address</th>
                    <th scope="col">Date of birth</th>
                    
                </tr>
            </thead>
            <tbody>
                {{#each content}}
                <tr>
                    
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                    <td>{{this.Phone}}</td>
                    <td>{{this.Address}}</td>
                    <td>{{this.DateOfBirth}}</td>
                    
                    
                    {{!-- <td><img style="width: 50px;height: 50px" src="/images/worker/{{this._id}}.png" alt=""></td> --}}
                    <td>
                        
                        <a href="/admin/deleteuser/{{this._id}}" class="btn btn-danger" onclick="return confirm('are you want to delete {{this.works.Name}} ?')">Delete</a>
                    </td>

                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>